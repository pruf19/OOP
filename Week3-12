import java.util.*;
public class week3_12 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int i;
        System.out.print("Please insert a number : ");
        int x = sc.nextInt();
        for (i = 1; i <= x; i++) {
            System.out.print("|");
            if (i % 5 == 0) {
                System.out.println();
            }
        }
    }
}
